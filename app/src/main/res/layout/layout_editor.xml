<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="#29354B"
		android:orientation="horizontal">
		<ImageView
			android:id="@+id/imageview1"
			android:layout_width="35dp"
			android:layout_height="35dp"
			android:layout_marginRight="20dp"
			android:src="@drawable/ic_arrow_back_white"
			android:scaleType="fitCenter" />
		<ImageView
			android:id="@+id/imageview2"
			android:layout_width="35dp"
			android:layout_height="match_parent"
			android:src="@drawable/ic_play_arrow_black"
			android:scaleType="fitCenter" />
		<ImageView
			android:id="@+id/imageview3"
			android:layout_width="35dp"
			android:layout_height="match_parent"
			android:src="@drawable/ic_save_sky_36dp"
			android:scaleType="fitCenter"
			android:tint="#ffffff" />
		<ImageView
			android:id="@+id/imageview5"
			android:layout_width="35dp"
			android:layout_height="match_parent"
			android:src="@drawable/ic_insert_drive_file_black"
			android:scaleType="fitCenter" />
		<ImageView
			android:id="@+id/imageview6"
			android:layout_width="35dp"
			android:layout_height="match_parent"
			android:src="@drawable/ic_unfold_more_black"
			android:scaleType="fitCenter" />
		<ImageView
			android:id="@+id/imageview7"
			android:layout_width="35dp"
			android:layout_height="match_parent"
			android:src="@drawable/icons8_click_64"
			android:scaleType="center" />
		<Spinner
			android:id="@+id/spinner1"
			android:layout_width="match_parent"
			android:layout_height="30dp"
			android:spinnerMode="dropdown" />
	</LinearLayout>
	<LinearLayout
		android:id="@+id/main"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="#29354B"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/linear5"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">
			<TextView
				android:id="@+id/nameT"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:text="document.body"
				android:textSize="12sp"
				android:textStyle="bold"
				android:textColor="#FFFFFF"
				android:layout_weight="1" />
			<CheckBox
				android:id="@+id/checkbox1"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:padding="5dp"
				android:gravity="center_horizontal|center_vertical"
				android:text="Drag"
				android:textSize="12sp"
				android:textStyle="bold"
				android:textColor="#FFFFFF" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/layout"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:padding="4dp"
			android:orientation="horizontal">
			<ListView
				android:id="@+id/widgets_list"
				android:layout_width="100dp"
				android:layout_height="match_parent"
				android:choiceMode="none" />
			<WebView
				android:id="@+id/webview1"
				android:layout_width="match_parent"
				android:layout_height="match_parent" />
		</LinearLayout>
		<ListView
			android:id="@+id/events"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:choiceMode="none" />
	</LinearLayout>
	<LinearLayout
		android:id="@+id/bottom"
		android:layout_width="match_parent"
		android:layout_height="180dp"
		android:padding="2dp"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/linear3"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">
			<Button
				android:id="@+id/button1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="5dp"
				android:padding="8dp"
				android:text="Properties"
				android:textSize="12sp"
				android:textStyle="bold"
				android:textColor="#FFFFFF" />
			<Button
				android:id="@+id/button2"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:text="Events"
				android:textSize="12sp"
				android:textStyle="bold"
				android:textColor="#FFFFFF" />
		</LinearLayout>
		<HorizontalScrollView
			android:id="@+id/hscroll1"
			android:layout_width="match_parent"
			android:layout_height="match_parent">
			<LinearLayout
				android:id="@+id/linear6"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:orientation="vertical">
				<GridView
					android:id="@+id/gridview1"
					android:layout_width="500dp"
					android:layout_height="match_parent"
					android:numColumns="30000"
					android:stretchMode="columnWidth"
					android:nestedScrollingEnabled="true" />
			</LinearLayout>
		</HorizontalScrollView>
	</LinearLayout>
</LinearLayout>
